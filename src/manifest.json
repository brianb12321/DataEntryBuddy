{
    "name": "Data Entry Buddy",
    "description": "Helps speed up data entry for DCTC",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "unlimitedStorage", "activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/assets/img/get_started16.png",
            "32": "/assets/img/get_started32.png",
            "48": "/assets/img/get_started48.png",
            "128": "/assets/img/get_started128.png"
        }
    },
    "icons": {
        "16": "/assets/img/get_started16.png",
        "32": "/assets/img/get_started32.png",
        "48": "/assets/img/get_started48.png",
        "128": "/assets/img/get_started128.png"
    }
}